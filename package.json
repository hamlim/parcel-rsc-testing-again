{
	"name": "parcel-react-server-starter",
	"private": true,
	"version": "0.0.0",
	"server": "dist/server.js",
	"source": "src/server.tsx",
	"targets": {
		"server": {
			"context": "react-server",
			"includeNodeModules": {
				"express": false,
				"hono": false,
				"@hono/node-server": false
			}
		}
	},
	"scripts": {
		"start": "parcel",
		"build:parcel": "parcel build",
		"wrangler:deploy": "bun wrangler deploy",
		"predeploy": "rm -rf dist .parcel-cache",
		"deploy": "bun build:parcel && bun wrangler:deploy"
	},
	"dependencies": {
		"@hono/node-server": "1.13.7",
		"@parcel/rsc": "canary",
		"express": "^4.21.2",
		"hono": "4.9.7",
		"react": "canary",
		"react-dom": "canary",
		"wrangler": "^3.105.0"
	},
	"devDependencies": {
		"@types/express": "^4",
		"@types/node": "^22.10.5",
		"@types/parcel-env": "^0.0.6",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"parcel": "canary",
		"@cloudflare/workers-types": "4.20250121.0"
	}
}
